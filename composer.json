{
    "name": "basereality/servercontainer",

    "description": "A poor mans docker.",

    "repositories": [
        { 
            "type": "composer", 
             "url": "http://satis.basereality.com"
        },
        {
          "type": "vcs",
          "url": "https://github.com/danack/auryn"
        }
    ],

    "require-dev": {
        "phpunit/phpunit": "4.4.5"
    },

    "require":{
        "aws/aws-sdk-php": "~2.7.12",
        "danack/auryn": "0.14.2",
        "danack/console": "~2.6.1",
        "danack/githubartaxservice": "0.4.3",
        "intahwebz/configurator": "^0.2.2"
    },
    "autoload": {
        "psr-0": {
            "ServerContainer": ["src/"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
